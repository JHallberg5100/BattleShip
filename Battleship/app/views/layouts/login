  <%= link_to "Home", games_path %>
    <% if logged_in? %>
      <%= link_to "#{current_user.username}", show_user_path %>
      <%= link_to "Logout", destroy_session_path, method: :destroy %>
    <% else %>
      <%= link_to "Register", new_user_path %>
      <%= link_to "Login", new_session_path %>
    <% end %>
